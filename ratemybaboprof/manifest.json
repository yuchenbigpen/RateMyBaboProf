{
  "name": "RateMyBaboProf",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Rate My Professor extension for Babson College",
  "homepage_url": "https://babson.edu",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": ["src/bg/background.js"]
  },
  "browser_action": {
    "default_icon": "icons/16.png"
  },
  "permissions": ["clipboardRead", "tabs", "activeTab"]
}
